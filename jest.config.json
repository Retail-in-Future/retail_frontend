{
    "testMatch": [
        "**/__tests__/**/*.spec.js"
    ],
    "moduleFileExtensions": [
        "js",
        "jsx"
    ],
    "moduleDirectories": [
        "node_modules",
        "src"
    ],
    "moduleNameMapper": {
        "\\.(css|scss|less|sass)$": "identity-obj-proxy",
        "\\.(ico|jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js"
    },
    "transform": {
        "^.+\\.js$": "babel-jest"
    }
}