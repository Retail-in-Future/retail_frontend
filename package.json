        "babel-plugin-module-resolver": "^2.7.1",
        "babel-plugin-transform-decorators-legacy": "^1.3.4",
        "babel-preset-es2015": "^6.22.0",
        "babel-preset-react": "^6.23.0",
        "babel-preset-stage-0": "^6.24.1",
        "babel-register": "^6.24.1",
        "cross-env": "^5.0.1",
        "css-loader": "^0.28.0",
        "enzyme": "^2.9.1",
