{
    "presets": [
        "es2015",
        "react",
        "stage-0"
    ],
    "plugins": [
        "transform-decorators-legacy",
        [
            "import",
            {
                "libraryName": "antd"
            }
        ]
    ],
    "env": {
        "test": {
            "plugins": [
                "transform-es2015-modules-commonjs",
                [
                    "module-resolver",
                    {
                        "root": [
                            "./src"
                        ],
                        "alias": {
                            "src": "./src"
                        }
                    }
                ]
            ]
        }
    }
}